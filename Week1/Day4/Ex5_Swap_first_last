import java.util.Arrays;
import java.util.Scanner;

public class ExX {
    public static void main(String[] args) {
        int[] nums = {2,3,4,5,6,21};

        int temp = nums[0];
        nums[0] = nums[nums.length-1];
        nums[nums.length-1] = temp;
        System.out.println(Arrays.toString(nums));
    }

}
