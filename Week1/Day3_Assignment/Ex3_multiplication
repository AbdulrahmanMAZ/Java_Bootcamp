import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        System.out.println("Enter Your number");
        int num = s.nextInt();
     
        for (int i = 0 ; i <= 10;i++){
            System.out.println(num*i);
        }




    }

}
